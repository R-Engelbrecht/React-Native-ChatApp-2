{
  "expo": {
    "name": "react-native-chatApp",
    "slug": "react-native-chatApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images//splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images//adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ruanjohannes.reactnativechatApp",
      "permissions": ["INTERNET"]
    },
    "extra": {
      "eas": {
        "projectId": "d1cba991-e480-4095-b68a-2a9493fd0989"
      }
    },
    "owner": "ruanjohannes",
    "plugins": [
      [
        "expo-sqlite",
        {
          "enableWeb": false
        }
      ]
    ]
  }
}